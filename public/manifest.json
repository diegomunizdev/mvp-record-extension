{
  "manifest_version": 3,
  "name": "Create React App Sample",
  "short_name": "React App",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "windows",
    "scripting",
    "desktopCapture",
    "tabCapture"
  ],
  "host_permissions": ["http://*/*", "https://*/*"]
}
