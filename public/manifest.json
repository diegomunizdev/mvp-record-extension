{
  "manifest_version": 3,
  "name": "Create React App Sample",
  "short_name": "React App",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "http://localhost:3001/*",
    "desktopCapture"
  ]
}
